{
  "expo": {
    "name": "SpillStack",
    "version": "1.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "scheme": "spillstack",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "xyz.create.spillstack",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSMicrophoneUsageDescription": "SpillStack needs microphone access to capture voice ideas",
        "NSCalendarsUsageDescription": "SpillStack can help you schedule ideas on your calendar",
        "NSCalendarsFullAccessUsageDescription": "SpillStack can help you schedule ideas on your calendar",
        "NSContactsUsageDescription": "SpillStack may use contacts to help you share ideas with others",
        "NSCameraUsageDescription": "SpillStack may use the camera to capture photos for your ideas",
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.732563511222-fj1fqguutuk88a8nb7ebasnj7pir5ldt"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "package": "xyz.create.spillstack"
    },
    "plugins": [
      [
        "expo-router",
        {
          "sitemap": false
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain"
        }
      ],
      "expo-audio",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "expo-video",
      "expo-web-browser",
      "@react-native-google-signin/google-signin",
      "expo-apple-authentication"
    ],
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false,
        "sitemap": false
      },
      "eas": {
        "projectId": "57bc8e03-7090-4f37-9003-cfc6f96acc7c"
      }
    },
    "runtimeVersion": "1.0.0",
    "owner": "thedamester"
  }
}
